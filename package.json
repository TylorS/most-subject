{
  "name": "most-subject",
  "version": "4.1.3",
  "description": "Subject-like interface for most.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TylorS/most-subject.git"
  },
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com> (https://github.com/TylorS)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mostjs-community/most-subject/issues"
  },
  "homepage": "https://github.com/mostjs-community/most-subject#readme",
  "config": {
    "ghooks": {
      "commit-msg": "node ./node_modules/.bin/validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": "@northbrook/commit-types"
    }
  },
  "scripts": {
    "clean": "rm -rf ./*.js ./*.js.map ./*.d.ts sync",
    "lint": "northbrook tslint",
    "test:unit": "northbrook ts-mocha",
    "test": "npm run lint && npm run test:unit",
    "build": "tsc",
    "commit": "northbrook commit",
    "preversion": "npm run build",
    "release": "northbrook release"
  },
  "devDependencies": {
    "@northbrook/preset-ts-mocha": "^1.2.1",
    "northbrook": "^2.2.6",
    "tslint": "^3.15.1",
    "typescript": "^2.0.0"
  },
  "peerDependencies": {
    "@most/multicast": "^1.2.3",
    "most": "^1.0.1"
  }
}
